{
  "name": "zyan-gulp-coveralls",
  "version": "0.1.7",
  "description": "Gulp plugin to submit code coverage to Coveralls",
  "keywords": ["gulpplugin", "coverage"],
  "homepage": "https://github.com/irockyan/gulp-coveralls",
  "bugs": "https://github.com/irockyan/gulp-coveralls/issues",
  "author": {
    "name": "zyan",
    "url": "https://github.com/irockyan"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:irockyan/gulp-coveralls.git"
  },
  "scripts": {
    "test":
      "eslint index.js && istanbul cover ./node_modules/mocha/bin/_mocha --report lcov -- -R spec",
    "coveralls":
      "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info && gulp coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "coveralls": "^3.0.0",
    "plugin-error": "^1.0.1",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "event-stream": "^3.3.0",
    "gulp": "^3.8.11",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "^13.2.1",
    "sinon": "^4.3.0"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://mit-license.org/irockyan"
    }
  ]
}
