{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "camelcase": "error",
    "curly": "error",
    "eqeqeq": "error",
    "indent": ["error", 2],
    "no-bitwise": "error",
    "no-caller": "error",
    "no-invalid-regexp": "error",
    "no-trailing-spaces": "warn",
    "no-undef": "error",
    "no-unused-vars": "error",
    "strict": "off"
  }
}
